{
  "name": "@webcoded/pgrestify",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript client library for PostgREST APIs with type safety and framework integrations",
  "keywords": [
    "postgrest",
    "postgresql",
    "rest-api",
    "typescript",
    "react",
    "nextjs",
    "tanstack-query",
    "client-library"
  ],
  "homepage": "https://pgrestify.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/pgrestify/pgrestify.git"
  },
  "bugs": {
    "url": "https://github.com/pgrestify/pgrestify/issues"
  },
  "author": "PGRestify Team",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "pgrestify": "./dist-cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./react": {
      "types": "./dist/adapters/react/index.d.ts",
      "import": {
        "types": "./dist/adapters/react/index.d.ts",
        "default": "./dist/adapters/react/index.mjs"
      },
      "require": {
        "types": "./dist/adapters/react/index.d.ts",
        "default": "./dist/adapters/react/index.cjs"
      },
      "default": "./dist/adapters/react/index.mjs"
    },
    "./tanstack-query": {
      "types": "./dist/adapters/tanstack-query/index.d.ts",
      "import": {
        "types": "./dist/adapters/tanstack-query/index.d.ts",
        "default": "./dist/adapters/tanstack-query/index.mjs"
      },
      "require": {
        "types": "./dist/adapters/tanstack-query/index.d.ts",
        "default": "./dist/adapters/tanstack-query/index.cjs"
      },
      "default": "./dist/adapters/tanstack-query/index.mjs"
    },
    "./nextjs": {
      "types": "./dist/adapters/nextjs/index.d.ts",
      "import": {
        "types": "./dist/adapters/nextjs/index.d.ts",
        "default": "./dist/adapters/nextjs/index.mjs"
      },
      "require": {
        "types": "./dist/adapters/nextjs/index.d.ts",
        "default": "./dist/adapters/nextjs/index.cjs"
      },
      "default": "./dist/adapters/nextjs/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "dist-cli",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "dev:types": "tsc --emitDeclarationOnly --watch",
    "dev:cli": "tsc cli/index.ts --outDir ./dist-cli --moduleResolution node --target ES2022 --module ESNext --esModuleInterop --skipLibCheck --watch",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm dev:types\" \"pnpm dev:cli\"",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cli": "tsc cli/index.ts --outDir ./dist-cli --moduleResolution node --target ES2022 --module ESNext --esModuleInterop --skipLibCheck",
    "build:all": "pnpm build && pnpm build:cli",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run src/__tests__/unit",
    "test:integration": "vitest --run src/__tests__/integration",
    "test:e2e": "vitest --run src/__tests__/e2e",
    "test:e2e:react": "vitest --run src/__tests__/e2e/react",
    "test:e2e:vanilla": "vitest --run src/__tests__/e2e/vanilla",
    "test:all-integrations": "pnpm test:e2e:react && pnpm test:e2e:vanilla",
    "test:cli": "npm run build:cli && node dist-cli/index.js --help",
    "typecheck": "tsc --noEmit",
    "typecheck:cli": "tsc cli/index.ts --noEmit --moduleResolution node --target ES2022 --module ESNext --esModuleInterop --skipLibCheck",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "size-limit": "size-limit",
    "docs:dev": "vitepress dev docs-site",
    "docs:build": "vitepress build docs-site",
    "docs:preview": "vitepress preview docs-site",
    "release": "semantic-release",
    "prepack": "npm run clean && npm run build:all",
    "prepublishOnly": "npm run build:all",
    "postpublish": "echo 'Package published successfully!'",
    "clean": "rm -rf dist dist-cli",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "@types/glob": "^9.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "inquirer": "^9.2.12",
    "node-sql-parser": "^4.17.0",
    "ora": "^7.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.20.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.62",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "semantic-release": "^23.0.2",
    "size-limit": "^11.0.2",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "^1.0.0-rc.44",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=4.0.0",
    "react": ">=16.8.0",
    "next": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "15 KB",
      "gzip": true
    },
    {
      "path": "dist/adapters/react/index.js",
      "limit": "5 KB",
      "gzip": true
    },
    {
      "path": "dist/adapters/tanstack-query/index.js",
      "limit": "3 KB",
      "gzip": true
    },
    {
      "path": "dist/adapters/nextjs/index.js",
      "limit": "15 KB",
      "gzip": true
    }
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}