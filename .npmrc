# PGRestify Development Configuration

# Enable peer dependency auto-installation (optional)
# auto-install-peers=true

# Enable strict peer dependency resolution
strict-peer-dependencies=false

# Enable legacy peer dependency resolution (for better compatibility)
legacy-peer-deps=false

# Enable package-lock.json (for npm users)
package-lock=true

# Enable pnpm-lock.yaml (for pnpm users)
lockfile-only=false

# Enable audit
audit=true

# Enable fund
fund=true

# Enable update-notifier
update-notifier=true

# Enable save-exact for development
save-exact=false

# Enable save-prefix
save-prefix=^ 